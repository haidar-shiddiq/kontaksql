<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kontaksql">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.KontakSql">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".membuat"
            android:label="@string/title_activity_membuat">
        </activity>
        <activity android:name=".melihat"
            android:label="@string/title_activity_melihat">
        </activity>
        <activity android:name=".mengupdate"
            android:label="@string/title_activity_mengupdate">
        </activity>
    </application>

</manifest>